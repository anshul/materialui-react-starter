{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "max-len": [2, { "code": 150 }],
  },
  "globals": {
    "it": true,
    "document": true
  }
}
